    <div class="col-sm-3">
      <input type="text" class="form-control" id="inputPassword4" placeholder="请输入手机号" name="phone">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-primary">注册</button>
    </div>
  </div>
</form>
</div>
